generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Achievement {
  id          String   @id @default(uuid())
  title       String
  description String
  option      String
  createdAt   DateTime @default(now())
  deletedAt   DateTime?
}

model User {
  id   String @id @default(uuid())
  name String
}
